<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.FenetreResultat">
   <center>
      <ListView BorderPane.alignment="CENTER" fx:id="lalisteJoueur"/>
   </center>
   <bottom>
      <BorderPane BorderPane.alignment="CENTER">
         <left>
            <Button mnemonicParsing="false" text="Accueil" BorderPane.alignment="CENTER" fx:id="boutonfermer" onAction="#clicRetour"/>
         </left>
         <right>
            <Button alignment="CENTER" mnemonicParsing="false" text="Rejouer" textAlignment="CENTER" BorderPane.alignment="CENTER" fx:id="boutonrejouer" onAction="#clicJouer"/>
         </right>
      </BorderPane>
   </bottom>
</BorderPane>
